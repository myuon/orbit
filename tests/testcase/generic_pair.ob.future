type Pair(A: type, B: type) = struct {
  first: A,
  second: B,

  fun set_first(self: Pair(A, B), a: A): int do
    self.first = a;
    return 0;
  end
};

fun main() do
  let p = new Pair(int, [*]byte) { .first = 1, .second = "hello, world" };
  p.set_first(3);

  let s = p.second;

  return p.first + (s[3] as int);
end